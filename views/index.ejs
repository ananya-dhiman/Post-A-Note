<html>
    <head>
        <title>POST A NOTE</title>
        <link rel="stylesheet" href="../styles.css">
        
    </head>
    <body>
        <% for(let i=0;i<messages.length;i++) { %>
            <li>
                <div class="note">
                <h2>Note</h2>
     
                    <div><%= messages[i].text %></div>
                    <div>Name - <%= messages[i].user %></div>
                    <div>Date - <%= messages[i].added %></div>
                </div>
            </li>
            
        
        <% } %>
    </body>

</html>